# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                         controllers.Application.index()


# Backend
GET        /backend/dashboard        controllers.backend.AddressBackendController.dashboard()
GET        /backend/address          controllers.backend.AddressBackendController.list()


#API
GET        /api/address/all          controllers.api.AddressAPIController.list()



# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file             controllers.Assets.at(path="/public", file)
